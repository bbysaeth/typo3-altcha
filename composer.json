{
    "name": "bbysaeth/typo3-altcha",
    "description": "TYPO3 form element for spam protection by utilizing the proof-of-work mechanism ALtcha.",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "abuse",
        "altcha",
        "bot",
        "captcha",
        "proof-of-work",
        "recaptcha",
        "spam",
        "spam-protection",
        "typo3"
    ],
    "homepage": "https://github.com/bbysaeth/typo3-altcha",
    "type": "typo3-cms-extension",
    "autoload": {
        "psr-4": {
            "BBysaeth\\Typo3Altcha\\": "Classes/"
        }
    },
    "authors": [
        {
            "name": "Benjamin Bys√§th",
            "email": "benjamin@bysaeth.de"
        }
    ],
    "require": {
        "typo3/cms-core": "^12.4.0 || ^13.0 || 13.0.x-dev",
        "typo3/cms-extbase": "^12.4.0 || ^13.0 || 13.0.x-dev",
        "typo3/cms-form": "^12.4.0 || ^13.0 || 13.0.x-dev",
        "typo3/cms-fluid": "^12.4.0 || ^13.0 || 13.0.x-dev",
        "altcha-org/altcha": "^1.1.2"
    },
    "suggest": {
        "typo3/cms-scheduler": "^12.4.0 || ^13.0 || 13.0.x-dev"
    },
    "extra": {
		"typo3/cms": {
			"extension-key": "altcha"
		}
	},
    "config": {
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    },
    "require-dev": {
        "helmich/typo3-typoscript-lint": "^3.2",
        "phpstan/phpstan": "^2.1"
    }
}
